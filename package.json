{
  "name": "@adonisjs/drive-gcs",
  "version": "0.0.0",
  "description": "Google cloud storage driver for AdonisJS drive",
  "main": "build/providers/GcsProvider.js",
  "files": [
    "build/adonis-typings",
    "build/instructions.md",
    "build/src",
    "build/providers"
  ],
  "types": "build/adonis-typings/index.d.ts",
  "scripts": {
    "mrm": "mrm --preset=@adonisjs/mrm-preset",
    "pretest": "npm run lint",
    "test": "node japaFile.js",
    "clean": "del build",
    "compile": "npm run lint && npm run clean && tsc",
    "build": "npm run compile",
    "prepublishOnly": "npm run build",
    "lint": "eslint . --ext=.ts",
    "format": "prettier --write .",
    "commit": "git-cz",
    "release": "np",
    "version": "npm run build",
    "sync-labels": "github-label-sync --labels ./node_modules/@adonisjs/mrm-preset/gh-labels.json adonisjs/drive-gcs"
  },
  "keywords": [
    "adonisjs",
    "adonisjs-drive",
    "adonis-drive",
    "gcs"
  ],
  "author": "virk,adonisjs",
  "license": "MIT",
  "devDependencies": {
    "@adonisjs/core": "^5.2.4",
    "@adonisjs/mrm-preset": "^4.1.2",
    "@adonisjs/require-ts": "^2.0.7",
    "@poppinss/dev-utils": "^1.1.5",
    "@types/node": "^16.6.1",
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "^3.3.0",
    "del-cli": "^4.0.1",
    "dotenv": "^10.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-adonis": "^1.3.3",
    "eslint-plugin-prettier": "^3.4.0",
    "github-label-sync": "^2.0.2",
    "got": "^11.8.2",
    "husky": "^7.0.1",
    "japa": "^3.1.1",
    "mrm": "^3.0.8",
    "np": "^7.5.0",
    "prettier": "^2.3.2",
    "typescript": "^4.3.5"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "np": {
    "contents": ".",
    "anyBranch": false
  },
  "peerDependencies": {
    "@adonisjs/core": "^5.0.0"
  },
  "adonisjs": {
    "instructionsMd": "./build/instructions.md",
    "env": {
      "GCS_KEY": "dummyKey",
      "GCS_SECRET": "dummySecret",
      "GCS_BUCKET": "dummyBucket",
      "GCS_REGION": "dummyRegion"
    },
    "types": "@adonisjs/drive-gcs",
    "providers": [
      "@adonisjs/drive-gcs"
    ]
  },
  "dependencies": {
    "@google-cloud/storage": "^5.13.1",
    "@poppinss/utils": "^3.2.0"
  }
}
