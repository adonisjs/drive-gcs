{
  "name": "@adonisjs/drive-gcs",
  "version": "1.0.2",
  "description": "Google cloud storage driver for AdonisJS drive",
  "main": "build/providers/GcsProvider.js",
  "files": [
    "build/adonis-typings",
    "build/instructions.md",
    "build/src",
    "build/providers"
  ],
  "types": "build/adonis-typings/index.d.ts",
  "scripts": {
    "mrm": "mrm --preset=@adonisjs/mrm-preset",
    "pretest": "npm run lint",
    "test": "node japaFile.js",
    "clean": "del build",
    "compile": "npm run lint && npm run clean && tsc && copyfiles instructions.md build",
    "build": "npm run compile",
    "prepublishOnly": "npm run build",
    "lint": "eslint . --ext=.ts",
    "format": "prettier --write .",
    "commit": "git-cz",
    "release": "np",
    "version": "npm run build",
    "sync-labels": "github-label-sync --labels ./node_modules/@adonisjs/mrm-preset/gh-labels.json adonisjs/drive-gcs"
  },
  "keywords": [
    "adonisjs",
    "adonisjs-drive",
    "adonis-drive",
    "gcs"
  ],
  "author": "virk,adonisjs",
  "license": "MIT",
  "devDependencies": {
    "@adonisjs/core": "^5.4.2",
    "@adonisjs/mrm-preset": "^5.0.2",
    "@adonisjs/require-ts": "^2.0.8",
    "@poppinss/dev-utils": "^2.0.0",
    "@types/node": "^17.0.0",
    "commitizen": "^4.2.4",
    "copyfiles": "^2.4.1",
    "cz-conventional-changelog": "^3.3.0",
    "del-cli": "^4.0.1",
    "dotenv": "^10.0.0",
    "eslint": "^8.5.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-adonis": "^2.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "github-label-sync": "^2.0.2",
    "got": "^12.0.0",
    "husky": "^7.0.4",
    "japa": "^4.0.0",
    "mrm": "^3.0.10",
    "np": "^7.6.0",
    "prettier": "^2.5.1",
    "typescript": "^4.5.4"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "np": {
    "contents": ".",
    "anyBranch": false
  },
  "peerDependencies": {
    "@adonisjs/core": "^5.4.2"
  },
  "adonisjs": {
    "instructionsMd": "./build/instructions.md",
    "env": {
      "GCS_KEY_FILENAME": "./google-cloud-key.json",
      "GCS_BUCKET": "bucketName"
    },
    "types": "@adonisjs/drive-gcs",
    "providers": [
      "@adonisjs/drive-gcs"
    ]
  },
  "dependencies": {
    "@google-cloud/storage": "^5.16.1",
    "@poppinss/utils": "^4.0.0"
  },
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/adonisjs/drive-gcs.git"
  },
  "bugs": {
    "url": "https://github.com/adonisjs/drive-gcs/issues"
  },
  "homepage": "https://github.com/adonisjs/drive-gcs#readme",
  "publishConfig": {
    "access": "public",
    "tag": "latest"
  }
}
